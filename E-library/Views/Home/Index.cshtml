@{
    ViewData["Title"] = "Home Page";
}
@await Html.PartialAsync("_Home")

<div class="text-center">
    <div class="wrap-element">
        <iframe class="wrapped-iframe" src="https://www.youtube.com/embed/RNEDEk1Tz4Y" title="YouTube video player"
                frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" 
        allowfullscreen></iframe>
    </div>
    <br />
    <br />
    <div>
        <h1 style="color:white" class="text-center h1">Game Mode</h1>
        <br />
        <br />
        <div class="box">
            <div class="box-inner">
                <img width="300" height="200" />
                <h3 class="box-inner-title">Replacing Books</h3>
                <h4 style="margin:20px">****</h4>
                <button>Locked</button>
            </div>
            <div class="box-inner">
                <img width="300" height="200" />
                <h3 class="box-inner-title">Identifying Areas</h3>
                <h5>****</h5>
                <button class="button">
                    <a class="nav-link text-white" asp-area="" asp-controller="Books" asp-action="Identify">Unlocked</a>
                </button>
            </div>
            <div class="box-inner">
                <img width="300" height="200" />
                <h3 class="box-inner-title">Finding Call Numbers</h3>
                <h4>****</h4>
                <button>Locked</button>
            </div>
        </div>
    </div>
    
    <br />
    <br />
    <div class="About">
        <h2 style="color:white" class="display-4">About Section</h2>
    </div>
    <br />
    <br />
    <div class="Support">
        <h2 style="color:white" class="display-4">Support Section</h2>
    </div>
</div>
